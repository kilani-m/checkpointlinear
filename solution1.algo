ALGORITHM solution1
VAR
  tab1 : ARRAY_OF INTEGER[20];
  tab2 : ARRAY_OF INTEGER[30];
  i,j,sum : INTEGER
BEGIN
read(tab1)
read(tab2)
sum :=0
FOR i FROM 0 TO tab1.length-1 STEP 1  DO
    FOR j FROM 0 TO length.tab2-1 STEP step  DO
        IF (tab1[i]=tab2[j]) THEN
            sum=sum+tab1[i]+tab2[j]
        END_IF
    END_FOR
END_FOR
ENDA
